#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
    string a,b,c;
    getline(cin,a);
    getline(cin,b);
    c=a+b;
    int l=a.size();
    int l1=b.size();
    cout<<l<<" "<<l1<<endl;
    cout<<c<<endl;
    char c1=a[0];
    char c2=b[0];
    a[0]=c2;
    b[0]=c1;
    cout<<a<<" "<<b;
  
    return 0;
}
